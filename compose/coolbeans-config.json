{
  "cluster": {
    "name": "garbanzo-beans",
    "in_mem": true,
    "raft_timeout": "30s",
    "restore_timeout": "30s",
    "retain_snasphot_count": 5,
    "max_pool": 3,
    "snapshot_threshold": 1024,
    "snapshot_interval": "30s",
    "trailing_logs": 1024
  },
  "nodes": [
    {
      "id": "bean0",
      "listen_addr": "bean0:11000",
      "raft_addr": "bean0:21000",
      "root_dir": "/tmp/bean0"
    },
    {
      "id": "bean1",
      "listen_addr": "bean1:12000",
      "raft_addr": "bean1:22000",
      "root_dir": "/tmp/bean1"
    },
    {
      "id": "bean2",
      "listen_addr": "bean2:13000",
      "raft_addr": "bean2:23000",
      "root_dir": "/tmp/bean2"
    }
  ]
}